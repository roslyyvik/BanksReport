<% layout('layouts/boilerplate') -%>
    <style>
        table,
        th,
        td {
            border: 1px solid grey;
            border-collapse: collapse;
        }
        
        th,
        td {
            padding: 10px;
            font-size: 17px;
        }
        
        canvas {
            background: #1f1c30;
        }
        
        #net_assets10_0,
        #total_liability_01,
        #total_capital_01,
        #total_profit_01 {
            background-image: linear-gradient(45deg, rgb(4, 4, 102) 50%, #6953e4);
        }
        
        .search {
            display: none
        }
        
        .jumbotron {
            opacity: 0.9;
            margin-bottom: 0;
            background-image: linear-gradient(to top right, rgb(4, 4, 102) 50%, #6953e4);
            /* min-height:calc(100vh - 91.6px); max-height:calc(var(--vh, 2.0vh) * 100); */
        }
        
        .jumbotron h1 {
            font-size: 4.5rem
        }
        
        .slider {
            width: 100%;
            margin: 0;
        }
        
        .slider-wrapper {
            width: 100%;
            height: 26vmax;
            position: relative;
        }
        
        .slide {
            float: left;
            position: absolute;
            width: 100%;
            opacity: 0;
            transition: opacity 3s linear;
        }
        
        .slider-wrapper>.slide:first-child {
            opacity: 1;
        }
        
        @media only screen and (max-width:992px) {
            .slider-wrapper {
                height: calc(100px + 26vmax)
            }
            .col-lg-2 {
                display: none;
            }
        }
        
        @media only screen and (max-width:420px) {
            .navbar {
                padding: 0.5rem 0rem
            }
            .navbar-expand .navbar-collapse {
                padding-right: 0px
            }
            .col-lg-7 {
                display: none
            }
            .jumbotron {
                height: calc(100vh - 91.6px);
                display: flex;
                justify-content: center
            }
            .jumbotron h1 {
                font-size: 4.1rem
            }
        }
        
        @media only screen and (max-width:280px) {
            .jumbotron h1 {
                font-size: 3.5rem
            }
        }
